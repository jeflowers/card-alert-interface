# Database Configuration
DB_USER=your_oracle_username
DB_PASSWORD=your_oracle_password
DB_CONNECT_STRING=your_oracle_connect_string

# Server Configuration
PORT=3000
NODE_ENV=development

# API Keys
API_KEY=your_api_key_for_authentication

# File Paths
SUSPECT_FILE_PATH=/path/to/suspect/files
BANDR_FILE_PATH=/path/to/bandr/files
FRAUD_FILE_PATH=/path/to/fraud/files

# Cron Schedule
SUSPECT_FILE_CRON="*/5 * * * *"  # Every 5 minutes
BANDR_FILE_CRON="0 */6 * * *"    # Every 6 hours
FRAUD_FILE_CRON="0 23 * * *"      # Once daily at 11 PM